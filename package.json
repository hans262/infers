{
  "name": "infers",
  "version": "1.2.9",
  "description": "Matrix operation and machine learning library by TypesScript.",
  "main": "./lib/index.js",
  "browser": "./lib/browser.js",
  "keywords": [
    "infers",
    "machine-learning",
    "neural-network",
    "robotics",
    "matrix",
    "math"
  ],
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc --project tsconfig.production.json",
    "get-registry": "npm get registry",
    "login": "npm login",
    "pub": "npm publish",
    "n-registry": "npm config set registry https://registry.npmjs.org",
    "t-registry": "npm config set registry https://registry.npm.taobao.org",
    "doc": "typedoc --options ./typedoc.json",
    "test": "jest",
    "esbuild": "esbuild src/browser.ts --bundle --outfile=browser/infers.esm.js --format=esm --minify",
    "serve": "serve browser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ounana/infers.git"
  },
  "author": "ounana",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "esbuild": "^0.11.4",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.3",
    "typedoc": "^0.20.32",
    "typescript": "^4.2.3"
  }
}