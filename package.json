{
  "name": "infers",
  "version": "1.1.5",
  "description": "Matrix operation and machine learning library by TypesScript.",
  "main": "./build/src/index.js",
  "keywords": [
    "machine-learning",
    "neural-network",
    "infers"
  ],
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "get-registry": "npm get registry",
    "login": "npm login",
