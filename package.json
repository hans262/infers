    "n-registry": "npm config set registry https://registry.npmjs.org",
    "t-registry": "npm config set registry https://registry.npm.taobao.org",
    "add-remote": "git remote add gitee https://gitee.com/badgua/infers.git",
    "push-gitee": "git push gitee main",
    "doc": "typedoc --options ./typedoc.json",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ounana/infers.git"
  },
  "author": "ounana",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.3",
    "typedoc": "^0.20.32",
    "typescript": "^4.2.3"
  }
}
